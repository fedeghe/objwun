{
    "name": "objwun",
    "version": "1.1.30",
    "main": "dist/index.js",
    "author": "fedeghe <fedeghe@gmail.com>",
    "license": "MIT",
    "description": "Isomorphic ES3 library to common stuff on object literals and arrays",
    "keywords": [
        "assign",
        "filter",
        "forEach",
        "get",
        "id",
        "includes",
        "isEmpty",
        "keyBy",
        "map",
        "omit",
        "pick",
        "reduce",
        "some",
        "sortBy",
        "times",
        "uniq"
    ],
    "repository": {
        "type": "git",
        "url": "https://fedeghe@github.com/fedeghe/objwun.git"
    },
    "devDependencies": {
        "coveralls": "~3.0.1",
        "jest": "^29.7.0",
        "malta": "^4.1.34",
        "malta-header-comment": "^1.0.12",
        "malta-js-uglify": "^1.0.13",
        "mocha-lcov-reporter": "~1.3.0",
        "nyc": "^15.1.0"
    },
    "scripts": {
        "build": "malta source/build.json",
        "dev": "malta source/build_dev.json",
        "test": "jest",
        "watch": "jest --watch",
        "lint": "eslint source",
        "cover": "jest --coverage",
        "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "json",
            "lcov",
            "html"
        ]
    }
}
