        "malta": "^4.1.19",
        "malta-header-comment": "^1.0.10",
        "malta-js-uglify": "^1.0.13",
        "malta-mocha": "^1.0.17",
        "mocha": "^8.1.1",
        "mocha-lcov-reporter": "~1.3.0",
        "nyc": "^15.1.0"
    },
    "scripts": {
        "build": "malta source/build.json",
        "buildev": "malta source/build_dev.json",
        "test": "mocha",
